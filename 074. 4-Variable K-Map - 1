//074. 4-Variable K-Map - 1

module top_module
    (
        input a,b, c, d,
        output out  
    ); 

    assign out = (((~a) | (~b) | c) & ((~b) | c | (~d)) & ((~a) | (~c) | d) & (a | b | (~c) | (~d)));
    
endmodule
